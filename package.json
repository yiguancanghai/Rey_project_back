{
  "name": "rey-portfolio-backend",
  "version": "1.0.0",
  "description": "Backend for Rey's AI Engineer Portfolio",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon",
    "test": "jest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "setup": "bash scripts/setup-dev.sh",
    "seed": "node src/scripts/seed.js",
    "docker:build": "docker build -t rey-portfolio-backend .",
    "docker:run": "docker run -p 5000:5000 rey-portfolio-backend",
    "docker:compose": "docker-compose up -d"
  },
  "dependencies": {
    "axios": "^1.8.4",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "validator": "^13.11.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3"
  }
}
